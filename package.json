{
  "name": "@rpg-sheet/server",
  "version": "1.0.0",
  "description": "RPG sheet that fits into any game.",
  "main": "index.js",
  "author": "Pedro Pinho",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/shared/infra/web/index.ts",
    "start:dev": "tsnd -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/web/index.ts",
    "typeorm": "tsnd -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:show": "yarn typeorm migration:show",
    "test": "jest"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^8.0.29",
    "@types/socket.io": "^2.1.10",
    "@types/uuid": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "celebrate": "^12.1.2",
    "class-transformer": "^0.2.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.9",
    "mysql": "^2.14.1",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.10",
    "socket.io": "^2.3.0",
    "tsyringe": "^4.3.0",
    "typeorm": "0.2.25",
    "uuid": "^8.3.0"
  }
}
